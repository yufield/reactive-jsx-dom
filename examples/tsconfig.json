{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES5",
    "jsx": "react",
    "jsxFactory": "Dom.h",
    "jsxFragmentFactory": "Dom.Fragment",
    "outDir": "./dist",
    "downlevelIteration": true,
    "skipLibCheck": true
  },
  "exclude": [
    "../node_modules/*"
  ],
  "include": [
    "./*"
  ],
}